    "scripts": {
        "prebuild": "rimraf dist",
        "dev": "pnpm prebuild && prisma generate && nest start -w",
        "stage:build": "prisma generate && nest build",
        "stage:boot": "nest start -w",
        "stage": "pnpm prebuild && pnpm stage:build && pnpm stage:boot",
        "prod:build": "swc src prisma --out-dir dist && prisma generate",
        "prod:boot": "node dist/src/main.js",
        "prod": "pnpm prebuild && pnpm prod:build && pnpm prod:boot",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "test": "NODE_ENV=test jest --forceExit",
        "coverage": "NODE_ENV=test jest --coverage; exit 0",
        "unit": "NODE_ENV=test vitest run test/unit",
        "integration": "NODE_ENV=test jest --forceExit test/integration",
        "e2e": "NODE_ENV=test jest --forceExit --config jest.e2e.config.json test/e2e"
    },
